{
  "expo": {
    "name": "Keep",
    "slug": "keep",
    "version": "1.0.0",
    "scheme": "keep-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "bundleIdentifier": "com.taulantshatri.keepapp",
      "appleTeamId": "N2ACMGX6D6",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "C617.1"
            ]
          }
        ]
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.taulantshatri.keepapp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "userInterfaceStyle": "dark",
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.taulantshatri.keepapp",
      "intentFilters": [
        {
          "action": "SEND",
          "data": {
            "mimeType": "text/plain"
          },
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-share-extension",
        {
          "name": "Keep",
          "bundleIdentifier": "com.taulantshatri.keepapp.ShareExtension",
          "main": "index.share.js",
          "appGroupIdentifier": "group.com.taulantshatri.keepapp",
          "activationRules": [
            {
              "type": "url",
              "max": 10
            },
            {
              "type": "text"
            },
            {
              "type": "image",
              "max": 10
            },
            {
              "type": "video",
              "max": 10
            },
            {
              "type": "file",
              "max": 10
            }
          ],
          "preprocessingFile": "preprocessing.js",
          "backgroundColor": {
            "red": 0,
            "green": 0,
            "blue": 0,
            "alpha": 0.4
          },
          "height": 650
        }
      ]
    ]
  }
}